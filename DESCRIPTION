Package: edeaR
Type: Package
Title: Exploratory and Descriptive Event-Based Data Analysis
Version: 0.9.3
Authors@R: c(person("Gert","Janssenswillen",email = "gert.janssenswillen@uhasselt.be", role = c("aut","cre")),
